{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
    "env": {
      "production": {
        "plugins": [
          "lodash",
          "transform-runtime",
          "transform-react-inline-elements",
          "transform-react-remove-prop-types"
        ]
      }
    }
}
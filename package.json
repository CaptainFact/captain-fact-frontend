{
  "name": "captain-fact-frontend",
  "version": "1.2.1",
  "private": true,
  "engines": {
    "npm": ">=8.19.2"
  },
  "scripts": {
    "build": "webpack-cli --config webpack.production.config.js",
    "prod:build": "webpack-cli --config webpack.production.config.js",
    "cypress": "cypress run",
    "cypress:open": "LANG=\"en_EN.UTF-8\" cypress open .",
    "dev": "webpack serve",
    "dev:build": "webpack-cli",
    "start": "npm run dev",
    "watch": "jest --watch",
    "test": "jest",
    "test:update": "jest -u",
    "wtest": "jest --watch",
    "coverage": "jest --coverage '--collectCoverageFrom=app/+(API|components|lib|state)/**/!(record).{js,jsx}'",
    "lint": "eslint --ext .js,.jsx app cypress",
    "lint:fix": "npm run lint -- --fix && npm run prettier",
    "prettier": "prettier --loglevel warn --write 'app/**/*.{js,jsx}' && prettier --write 'cypress/**/*.js'",
    "prettier:check": "prettier --loglevel warn --check 'app/**/*.{js,jsx}' && prettier --check 'cypress/**/*.js'"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      ".history",
      "cypress"
    ]
  },
  "browserslist": {
    "production": [
      "> 2%",
      "not ie <=10",
      "not ie_mob <=11",
      "not bb <=10",
      "not op_mob <= 37",
      "not samsung <= 7"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@popperjs/core": "2.11.8",
    "@rebass/grid": "~6.0.0-5",

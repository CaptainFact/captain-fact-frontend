    "bulma": "^0.3.2",
    "classnames": "^2.2.5",
    "core-js": "^2.5.3",
    "date-fns": "^1.28.5",
    "debounce": "^1.0.2",
    "debounce-promise": "^3.0.2",
    "diff": "^3.3.1",
    "i18next": "^10.2.2",
    "immutable": "^4.0.0-rc.9",
    "is-promise": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "phoenix": "^1.3.0",
    "prop-types": "^15.6.0",
    "re-reselect": "^1.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-flip-move": "^2.10.2",
    "react-i18next": "^7.3.1",
    "react-markdown": "^3.1.4",
    "react-player": "^0.25.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.1",
    "react-select": "^1.2.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-form": "^6.7.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "smoothscroll-polyfill": "^0.4.0",
    "uuid": "^3.0.1",
    "validator": "^9.2.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.0.0",
    "babel-brunch": "^6.1.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.6",

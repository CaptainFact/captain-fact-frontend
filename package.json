{
  "name": "captain-fact-frontend",
  "version": "0.8.7",
  "private": true,
  "scripts": {
    "build": "webpack-cli --config webpack.production.config.js",
    "dev": "webpack-dashboard -- webpack-dev-server",
    "serve": "webpack-dashboard -- webpack-dev-server",
    "start": "webpack-dashboard -- webpack-dev-server",
    "watch": "jest --watch",
    "test": "jest",
    "wtest": "jest --watch",
    "coverage": "jest --coverage '--collectCoverageFrom=app/+(API|components|lib|state)/**/!(record).{js,jsx}'",
    "lint": "./node_modules/.bin/eslint app",
    "lint_all": "./node_modules/.bin/eslint app/**/*.jsx",
    "lint_fix": "./node_modules/.bin/eslint app --fix"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js",
      "jest-localstorage-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },

{
  "name": "captain-fact-frontend",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "build": "npx webpack-cli --config webpack.production.config.js",
    "prod:build": "npx webpack-cli --config webpack.production.config.js",
    "cypress": "npx cypress run",
    "cypress:open": "LANG=\"en_EN.UTF-8\" npx cypress open .",
    "dev": "npx webpack-dev-server",
    "dev:build": "npx webpack-cli",
    "dev:network": "npm run dev -- --host 0.0.0.0 --disable-host-check",
    "start": "npm run dev",
    "watch": "npx jest --watch",
    "test": "npx jest",
    "test:update": "npx jest -u",
    "wtest": "npx jest --watch",
    "coverage": "npx jest --coverage '--collectCoverageFrom=app/+(API|components|lib|state)/**/!(record).{js,jsx}'",
    "lint": "npx eslint app/**/*.{js,jsx}",
    "lint_fix": "npm run lint -- --fix",
    "prettier": "npx prettier --write app/**/*.{js,jsx} && npx prettier --write cypress/**/*.js",
    "prettier:check": "npx prettier --check app/**/*.{js,jsx} && npx prettier --check cypress/**/*.js",
    "styleguide:dev": "npx styleguidist server",
    "styleguide:build": "npx styleguidist build"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js",
      "jest-localstorage-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      ".history",
      "cypress"
    ]
  },
  "browserslist": {
    "production": [
      "> 2%",
      "not ie <=10",
      "not ie_mob <=11",
      "not bb <=10",
      "not op_mob <= 37",
      "not samsung <= 7"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "~7.11.5",
    "@rebass/grid": "~6.0.0-5",
    "@styled-icons/bootstrap": "^10.19.0",
    "animate.scss": "0.0.6",

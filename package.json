    "immutable": "~4.0.0-rc.12",
    "is-promise": "~2.1.0",
    "isomorphic-fetch": "~2.2.1",
    "lodash": "^4.17.20",
    "phoenix": "~1.4.2",
    "polished": "^3.4.2",
    "prop-types": "~15.7.2",
    "re-reselect": "~3.4.0",
    "react": "~16.9.0",

    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.22.0",
    "brunch": "2.10.9",
    "clean-css-brunch": "^2.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "gzip-brunch": "^1.3.0",
    "html-pages-brunch": "^2.6.0",
    "jest": "^22.0.6",
    "jest-localstorage-mock": "^2.2.0",
    "jsenv-brunch": "^2.0.0",
    "json-brunch": "^1.5.4",

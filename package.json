{
  "name": "captain-fact-frontend",
  "version": "1.2.1",
  "private": true,
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=18.20.6"
  },
  "scripts": {
    "build": "webpack-cli --config webpack.production.config.js",
    "prod:build": "webpack-cli --config webpack.production.config.js",
    "cypress": "cypress run",
    "cypress:open": "LANG=\"en_EN.UTF-8\" cypress open .",
    "dev": "webpack serve",
    "dev:build": "webpack-cli",
    "start": "npm run dev",
    "watch": "jest --watch",
    "test": "jest",
    "test:update": "jest -u",
    "wtest": "jest --watch",
    "coverage": "jest --coverage '--collectCoverageFrom=app/+(API|components|lib|state)/**/!(record).{js,jsx}'",
    "lint": "eslint --ext .js,.jsx app cypress",
    "lint:fix": "npm run lint -- --fix && npm run prettier",
    "prettier": "prettier --loglevel warn --write 'app/**/*.{js,jsx}' && prettier --write 'cypress/**/*.js'",
    "prettier:check": "prettier --loglevel warn --check 'app/**/*.{js,jsx}' && prettier --check 'cypress/**/*.js'",
    "ts-unused-exports": "ts-unused-exports tsconfig.json --ignoreFiles='app/constants.js' --ignoreFiles='/components/ui/*'",
    "typescript": "tsc --noEmit"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png)$": "<rootDir>/src/__mocks__/fileMock.js",
      "^config$": "<rootDir>/config/test.js",
      "^@/components/(.*)$": "<rootDir>/app/components/$1",
      "^@/lib/(.*)$": "<rootDir>/app/lib/$1",
      "^@/styles/(.*)$": "<rootDir>/app/styles/$1",
      "^@/hooks/(.*)$": "<rootDir>/app/hooks/$1",
      "^@/ui/(.*)$": "<rootDir>/app/components/ui/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      ".history",
      "cypress"
    ]
  },
  "browserslist": {
    "production": [
      "> 2%",
      "not ie <=10",
      "not ie_mob <=11",
      "not bb <=10",
      "not op_mob <= 37",
      "not samsung <= 7"
    ]
  },

{
  "name": "captain-fact-frontend",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "cypress": "npx cypress run",
    "cypress:open": "npx cypress open",
    "dev": "next -p 3333",
    "start": "next start",
    "watch": "npx jest --watch",
    "test": "npx jest",
    "test:update": "npx jest -u",
    "wtest": "npx jest --watch",
    "coverage": "npx jest --coverage '--collectCoverageFrom=app/+(API|components|lib|state)/**/!(record).{js,jsx}'",
    "lint": "npx eslint app",
    "lint_all": "npx eslint app/**/*.jsx",
    "lint_fix": "npx eslint app --fix",
    "prettify": "npx prettier --write app/**/*.{js,jsx} && npx prettier --write cypress/**/*.js",
    "styleguide:dev": "npx styleguidist server",
    "styleguide:build": "npx styleguidist build"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js",
      "jest-localstorage-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      ".history"
    ]
  },
  "lint-staged": {
    "app/**/*.{js,jsx}": [
      "npx prettier --write",
      "git add",
      "npx eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      "> 2%",
      "not ie <=10",
      "not ie_mob <=11",
      "not bb <=10",
      "not op_mob <= 37",
      "not samsung <= 7"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "~7.4.0",
    "@rebass/grid": "~6.0.0-5",
    "@zeit/next-sass": "^1.0.1",
    "animate.scss": "0.0.6",
    "apollo-boost": "~0.1.12",
    "bulma": "~0.7.2",
    "classnames": "~2.2.6",
    "clean-tag": "^2.0.2",
    "core-js": "~2.6.1",
    "date-fns": "~1.30.1",
    "debounce": "~1.0.2",
    "debounce-promise": "~3.1.0",
    "diff": "~3.3.1",
    "express": "^4.17.1",
    "faker": "~4.1.0",
    "formik": "^1.4.1",
    "graphql": "~14.0.2",
    "graphql-tag": "~2.9.2",
    "i18next": "~13.1.0",
    "immutable": "~4.0.0-rc.9",
    "is-promise": "~2.1.0",
    "isomorphic-fetch": "~2.2.1",
    "lodash": "^4.17.13",
    "next": "^9.0.2",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^0.47.0",
    "next-images": "^1.1.2",
    "phoenix": "~1.4.2",
